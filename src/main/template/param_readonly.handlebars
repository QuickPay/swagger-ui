<td class='code {{indent}}'>{{name}}</td>
<td>
{{#if showInput}}
  {{#if isBody}}
    <textarea class='body-textarea' readonly='readonly' name='{{name}}'>{{defaultValue}}</textarea>
  {{else}}
    {{#if defaultValue}}
      {{defaultValue}}
    {{else}}
      (empty)
    {{/if}}
  {{/if}}
{{/if}}
</td>
<td>{{{description}}}</td>
<td>{{{paramType}}}</td>
<td><span class="model-signature"></span></td>
